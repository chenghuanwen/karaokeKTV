         android:textSize="30sp"
		 android:textColor = "#ffffff"
		 android:text="@string/left"
		 android:visibility="gone"/>
	<TextView
        android:id="@+id/open_start"
        android:layout_width="100dip"
		android:layout_toRightOf ="@+id/open_start_l"
