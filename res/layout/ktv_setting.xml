<RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:gravity="center"
        android:layout_marginLeft="50dp">	
    
	<TextView
        android:id="@+id/boot_audio_tx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
		android:textColor = "#ffffff" 
		android:maxLength="25"
		android:textSize ="20sp"	
        />
    <Button
         android:id="@+id/audio_down"
         android:layout_width="41dp"
         android:layout_height="41dp"
		 android:layout_toRightOf ="@+id/boot_audio_tx"
		 android:background="@drawable/btn_key_selecter3"
         android:textSize="20sp"
          android:focusable="true"
		 android:focusableInTouchMode="true" 
		 android:textColor = "@drawable/btn_new_textcolor_selector"
		 android:text="@string/down"/>
	<TextView
        android:id="@+id/audio_num"
        android:layout_width="wrap_content"
		 android:layout_toRightOf ="@+id/audio_up"
        android:layout_height="wrap_content"
		android:gravity ="center"
		android:textColor = "#ffffff" 
		android:maxLength="25"
		android:textSize ="20sp"	
        android:layout_centerVertical="true"/>
    <Button
         android:id="@+id/audio_up"
         android:layout_width="41dp"
         android:layout_height="41dp"
		 android:layout_toRightOf ="@+id/audio_down"
		 android:background="@drawable/btn_key_selecter3"
         android:textSize="20sp"
          android:focusable="true"
		 android:focusableInTouchMode="true" 
		 android:textColor = "@drawable/btn_new_textcolor_selector"
		 android:text="@string/up"
		 android:layout_marginLeft="20dp"/>
</RelativeLayout>
<RelativeLayout
